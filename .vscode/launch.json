{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Pagination",
      "protocol": "inspector",
      "args": [
        "${workspaceRoot}/packages/utilities/examples/pagination/main.ts"
      ],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Slash",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/examples/slash/main.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register"],
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Guards",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/examples/guards/main.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register"],
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Event",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/examples/event/main.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register"],
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Multiple discord instances",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/examples/multiple-discord-instances/main.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register"],
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Debug tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "--runInBand",
        "--config",
        "${workspaceRoot}/jest.config.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    }
  ]
}
